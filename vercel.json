{
  "redirects": [
    { "source": "/:path*.html", "destination": "/404", "permanent": false }
  ],
  "rewrites": [
    { "source": "/", "destination": "/home" },
    { "source": "/home", "destination": "/index.html" },
    { "source": "/home/search", "destination": "/search.html" },
    { "source": "/home/about-us", "destination": "/about.html" },
    { "source": "/home/terms-of-service", "destination": "/terms.html" },
    { "source": "/home/privacy-policy", "destination": "/privacy.html" },
    { "source": "/home/faq", "destination": "/faq.html" }
  ]
}
